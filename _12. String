#include <iostream>
#include <string>
using namespace std;

int main() {
	// Complete the program
    string a,b;
    cin>>a;
    cin>>b;
    cout<<a.size()<<" "<<b.size()<<endl;
    string c=a+b;
    cout<<c<<endl;
    char temp;
    temp=b[0];
    b[0]=a[0];
    a[0]=temp;
    //cout<<temp;
    cout<<a<<" "<<b<<endl;
    return 0;
}
