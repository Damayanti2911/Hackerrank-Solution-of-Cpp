#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */ 
    int n,q,m,s=0;  
    cin>>n>>q;
    int* a[n];
    while(n--)
    {
        int m;
        cin>>m;
        a[s]=new int[m];
        for(int i=0;i<m;i++)
        {
            cin>>a[s][i];
        }
        s++;
    }
    while(q--)
    {
        int x,y;
        cin>>x>>y;
        cout<<a[x][y]<<endl;
    }
    


    
    return 0;
}
