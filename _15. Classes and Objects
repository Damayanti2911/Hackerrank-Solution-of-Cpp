#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
#include <cassert>
using namespace std;

// Write your Student class here
class Student
{
    public:
    int score[5];
    void input()
    {
       
        for (int i=0;i<5;i++)
        {
            cin>>score[i];
        }
        
    }
    int calculateTotalScore()
    {
        int grade=0;
        for(int i=0;i<5;i++)
        {
            grade+=score[i];
        }
        return grade;
    }
};
